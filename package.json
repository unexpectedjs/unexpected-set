{
  "name": "unexpected-set",
  "version": "1.1.0",
  "description": "Add support to Unexpected for testing Set instances",
  "main": "lib/unexpected-set.js",
  "scripts": {
    "lint": "eslint .",
    "test": "mocha && npm run lint && mocha documentation",
    "travis": "npm test && npm run coverage && (<coverage/lcov.info coveralls || true)",
    "coverage": "NODE_ENV=development istanbul cover _mocha -- --reporter dot && echo google-chrome coverage/lcov-report/index.html",
    "generate-site": "generate-site --require ./bootstrap-unexpected-markdown.js",
    "update-examples": "generate-site --require ./bootstrap-unexpected-markdown.js --update-examples",
    "deploy-site": "deploy-site.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unexpectedjs/unexpected-set.git"
  },
  "keywords": [
    "test",
    "assertion",
    "Unexpected",
    "Set",
    "array"
  ],
  "author": "Andreas Lind <andreas@one.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/unexpectedjs/unexpected-set/issues"
  },
  "homepage": "https://unexpected.js.org/unexpected-set",
  "devDependencies": {
    "coveralls": "2.11.8",
    "eslint": "2.13.1",
    "eslint-config-onelint": "1.2.0",
    "istanbul": "0.4.2",
    "mocha": "2.4.5",
    "unexpected": "10.10.0",
    "unexpected-documentation-site-generator": "^4.0.0",
    "unexpected-markdown": "^1.3.4"
  },
  "dependencies": {
    "es6-set": "0.1.4"
  }
}
